<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

<package name="input" extends="struts-default">
<action name="input"  class="com.tutorialspoint.struts2.action.InputAction" method="execute">
<result name="success">Home.jsp</result>
<result name="failure">Failure.jsp</result>
</action>


 <action name="output"
          class="com.tutorialspoint.struts2.action.OutputAction" 
          method ="execute">
    
      <result name="success">/Output.jsp</result>
       <result name="failed">Failure.jsp</result>
      
      </action>
 
</package>
</struts>